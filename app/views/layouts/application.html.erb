<!DOCTYPE html>
<html>
  <head>
    <title>Quora</title>
    

    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%#= javascript_importmap_tags %>
    <%= javascript_include_tag :application %>
    <%= javascript_include_tag Ckeditor.cdn_url %>

  </head>
  <body>
    <header class="navbar navbar-fixed-top navbar-inverse  bg-dark" style="background-color: black;">
      <div class="container">

    <%= link_to "home", root_path  %>
  <% if user_signed_in? %>
    


    <%= link_to "Show my questions",   new_question_path %>
    <%= link_to "Show all topics",   topics_path  %>
    <%= link_to "Show my answers",   answers_path  %>
    <%= link_to "Profile",   profile_path(id: current_user.id)  %>
    <%= link_to "Users",   profile_index_path   %>
    <%= link_to "Log OUT",   destroy_user_session_path, method: :delete  %>
    <%#= button_to("Log Out",   destroy_user_session_path, method: :delete) %>  
  <% else %>
    <%= link_to "log IN", new_user_session_path  %>
    <%= link_to "Sing UP", new_user_registration_path  %>
  <% end %>
    </div><br><br><br><br>

</header>
    <%#= debug(params) if Rails.env.development? %>
    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-danger"><%= alert %></p>
    <% end %>
      <%# flash.each do |name, msg| %>
         <%#= msg %>
       <%# end %>
    <%= yield %>    <br>
    <br>
  <footer class="navbar fixed-bottom navbar-inverse  bg-dark"> 
    

  </footer>

  </body>
</html>
